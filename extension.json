{
    "identifier": "Mecham.TreeCompare",
    "name": "Tree Compare",
    "author": "Justin Mecham",
    "description": "Lorem ipsum, dolor sit amet.",
    "version": "1.0",
    "main": "main.js",
    "entitlements": {
        "process": true
    },
    "sidebars": [
        {
            "id": "treeCompare",
            "name": "Tree Compare",
            "imageName": "treecompare",
            "sections": [
                {
                    "id": "treeCompare",
                    "name": "Branch Changes",
                    "headerCommands": [
                        {
                            "title": "Target Branch",
                            "image": "__builtin.branch",
                            "tooltip": "Change target branch for compare",
                            "command": "treeCompare.changeBranch",
                            "when": "false"
                        },
                        {
                            "title": "Refresh",
                            "image": "__builtin.refresh",
                            "tooltip": "Refresh working copy",
                            "command": "treeCompare.refresh"
                        }
                    ],
                    "contextCommands": [
                        {
                            "title": "Remove",
                            "command": "treeCompare.remove",
                            "when": "viewItem != null"
                        },
                        {
                            "title": "Show in Finder",
                            "command": "treeCompare.showInFinder",
                            "when": "viewItem != null"
                        }
                    ]
                }
            ]
        }
    ]
}
